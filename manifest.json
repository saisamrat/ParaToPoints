{
	"manifest_version": 2,

	"name":"ParaToPoints",
	"description": "select paragraph text and right click and select ParaToPoints to 
		split to points",
	"version": "1.0",

	"background":{
	"scripts": ["start.js"]
	},
	
	"browser_action":{
		"default_icon":"icon.png",
		"default_popup":"index.html"
	},

	"icons": {
          "16": "icon.png"
        },

	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",

	"permissions":[
		"storage",
		"contextMenus"
	]
}